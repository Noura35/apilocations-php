{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [

    {
      "src": "/",
      "dest": "/"
    },
    
    {
      "src": "/api/.*",
      "dest": "/api/index.php"
    },


    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "APP_KEY": "base64:hnZsG1j2FWF31ykcQoQr+pqT6frNOOUKE2rFrInNhdA=",
    "DB_CONNECTION": "databaselocations",
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
